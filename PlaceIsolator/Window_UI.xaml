<Window x:Class="SOM.RevitTools.PlaceIsolator.Window_UI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Window_UI" Height="250" Width="350">
    <Grid>
        <Button x:Name="selectFile" Content="Select File" Margin="131,10,136,0" VerticalAlignment="Top" Click="selectFile_Click"/>
        <ComboBox x:Name="LevelComboBox" HorizontalAlignment="Right"  Margin="0,92,10,0" Width="322" SelectionChanged="ComboBox_SelectionChanged" Height="25" VerticalAlignment="Top"/>
        <Button x:Name="okButton" Content="Create Isolator" Margin="0,0,102,10" Click="okButton_Click" Height="22" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="97" />
        <Button x:Name="cancelButton" Content="Cancel" Margin="0,0,22,10" Click="cancelButton_Click" Height="22" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" />
        <Label Content="Select Level" HorizontalAlignment="Left" Margin="10,61,0,0" VerticalAlignment="Top" FontSize="16" FontFamily="Helvetica"/>
        <Label Content="Select Excel" HorizontalAlignment="Left" Margin="10,6,0,136" FontSize="16" FontFamily="Helvetica"/>
    </Grid>
</Window>
